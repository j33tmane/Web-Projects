<p>    setTimeout(function()</p>
<p>    {</p>
<p>        document.getElementById(&#8216;myslider&#8217;).src = ListImages[startimage];</p>
<p>        document.getElementById(&#8216;myslider&#8217;).className = &#8220;&#8221;;</p>
<p>    },2000);</p>
<p>    startimage=startimage + 1;</p>
<p>    if (startimage == ListImages.length)</p>
<p>    {</p>
<p>    startimage = 0;</p>
<p>    }</p>
<p>    setTimeout(slideShow,Duration);</p>
<p>    }</p>
<p>   slideShow();</p>
<p>   }</p>
